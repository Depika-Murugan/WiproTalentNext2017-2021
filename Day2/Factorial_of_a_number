// an algorithm to calculate the factorial of a number N. The value of N is provided as an input by the user. 
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner s=new Scanner(System.in); 
	    int n=s.nextInt();
	    int fact=1;
	    while(n!=0)
	    {
	        fact*=n;
	        n--;
	    }
	    System.out.println(fact);
	 }
}
